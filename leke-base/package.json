{
  "name": "leke",
  "version": "1.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "website"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "guoquankai@cnstrong.cn",
  "license": "MIT",
  "scripts": {
    "build:icons": "cd packages/icons && npm run build",
    "build:hooks": "cd packages/hooks && npm run build",
    "build:http": "cd packages/http && npm run build",
    "build:components": "cd packages/rc && npm run build",
    "build:mobile": "cd packages/mobile && npm run build",
    "build:store": "cd packages/store && npm run build",
    "build:ssr": "cd packages/ssr && npm run build",
    "build:AV": "cd packages/AV && npm run build",
    "build:JSComponents": "cd packages/JSComponents && npm run build",
    "build:website": "cd website && npm run build",
    "build:rc": "npm-run-all --serial build:http build:AV build:JSComponents build:icons build:hooks build:components",
    "dev:website": "cd website && npm run dev",
    "dev": "npm-run-all --serial build:icons dev:website",
    "build:site": "npm-run-all --serial build:icons build:website",
    "test": "jest --cache false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.md",
    "fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.md",
    "tsc": "tsc --noEmit --jsx react",
    "eslint": "eslint",
    "major": "standard-version -r major",
    "minor": "standard-version -r minor",
    "patch": "standard-version -r patch --skip.tag"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-spread": "^7.12.1",
    "@babel/plugin-transform-template-literals": "^7.12.1",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.2.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-jest": "^26.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-less": "^4.0.1",
    "gulp-merge": "^0.1.1",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-raw-loader": "^1.0.1",
    "less": "^3.11.1",
    "lint-staged": "^10.5.1",
    "merge2": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "standard-version": "^9.3.1"
  }
}
